        <swagger.version>1.5.8</swagger.version>
        <servlet-api.version>2.5</servlet-api.version>

        <cxf.version>2.7.16</cxf.version>
        <jsr311.version>1.1.1</jsr311.version>
        <javax.ws.rs.version>2.0.1</javax.ws.rs.version>
